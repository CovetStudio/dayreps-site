{
  "require": {
    "craftcms/cms": "^3.0.0",
    "fatfish/navigation": "1.0.7",
    "markhuot/craftql": "1.3.2",
    "studioespresso/craft-navigate": "2.2.0",
    "trendyminds/buster": "1.0.6.1",
    "verbb/navigation": "1.1.11",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
